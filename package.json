{
  "name": "graphql-ts-server",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "gql2ts": "^1.10.1",
    "graphql-request": "^5.1.0",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "tslint": "^6.1.3",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.5.14",
    "@graphql-tools/load": "^7.8.10",
    "@graphql-tools/load-files": "^6.6.1",
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.16",
    "graphql-yoga": "^3.4.0",
    "mongodb": "^3.0.8",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.11"
  },
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "gen-schema-type": "gql2ts src/schema.graphql -o src/types/graphql.d.ts",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
